{
    "JwtSettings": {
        "SecretKey": "",
        "ExpiryMinutes": 60,
        "Issuer": "Lumina",
        "Audience": "Lumina"
    },
    "EncryptionSettings": {
        "SecretKey": "" // base64 string
    },
    "Serilog": {
        "WriteTo": [
            {
                "Name": "Debug"
            },
            {
                "Name": "Console"
            },
            {
                "Name": "File",
                "Args": {
                    "path": "Logs/lumina_api_.log",
                    "rollingInterval": "Day",
                    "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
                }
            }
        ]
    },
    "AllowedHosts": "*"
}
