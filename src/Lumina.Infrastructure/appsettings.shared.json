{
    "MediaSettings": {
        "RootDirectory": "wwwroot/media"
    },
    "CommonSettings": {
        "Theme": "LuminaDark"
    },
    "ConnectionStrings": {
        "DefaultConnection": "Server=<host>;Database=<database>;User Id=<user>;Password=<password>;"
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft.EntityFrameworkCore.Database.Command": "Debug"
            }
        },
        "Enrich": [
            "FromLogContext",
            "WithMachineName",
            "WithThreadId"
        ]
    }
}
